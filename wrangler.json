{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist/client",
    "run_worker_first": true,
  },
  "compatibility_date": "2025-12-02",
  "compatibility_flags": [
    "nodejs_als"
  ],
  "dev": {
    "port": 8080
  },
  "main": "./dist/server/index.js",
  "name": "solarflare",
  "vars": {
    "BUN_VERSION": "1.3.4",
    "HELLO": "WORLD",
    "SKIP_DEPENDENCY_INSTALL": 1
  }
}
