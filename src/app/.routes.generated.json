{
  "routes": [
    {
      "pattern": "/blog/:slug",
      "tag": "sf-blog-slug",
      "chunk": "/blog.slug.js",
      "styles": [
        "/blog.css"
      ],
      "type": "client",
      "params": [
        "slug"
      ]
    },
    {
      "pattern": "/",
      "tag": "sf-root",
      "chunk": "/index.js",
      "type": "client",
      "params": []
    },
    {
      "pattern": "/api",
      "tag": "sf-api",
      "type": "server",
      "params": []
    }
  ]
}