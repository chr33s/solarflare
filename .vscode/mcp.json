    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--browser=chrome", "--extension"]
    },
    "preact": {
      "type": "sse",
      "url": "https://gitmcp.io/preactjs/preact"
    }
  }
}
